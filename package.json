{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "prod": "node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "tsc": "tsc",
    "build": "npx tsc",
    "test": "npm run test:drop && npm run test:create && npm run test:migrate && npm run test:jasmine && npm run test:drop",
    "test:create": "db-migrate db:create udacity_fsjs_project2_test --env test_create --verbose",
    "test:drop": "db-migrate db:drop udacity_fsjs_project2_test --env test_create --verbose",
    "test:migrate": "db-migrate up --env test --verbose",
    "test:jasmine": "cross-env ENV=test jasmine-ts --config=jasmine.json",
    "tslint": "eslint ./ --ext .ts --fix"
  },
  "author": "jgrindall",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "supertest": "^6.1.5",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
